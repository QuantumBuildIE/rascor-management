			GetSite as "GetSite"
			{
				type =  form
				form = SPA
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											// Debug: Check what location data we're receiving
										info "Location String: " + input.Added_Location;
										// Get the created location - it's stored as a string
										locationString = input.Added_Location;
										// Parse latitude and longitude from the location string
										if(locationString != null)
										{
											try 
											{
												// Get and trim the parts to remove any whitespace
												latString = input.Added_Location.latitude;
												lonString = input.Added_Location.longitude;
												createdLat = latString.toDecimal();
												createdLon = lonString.toDecimal();
												// Debug: Check parsed coordinates
												info "Created Lat: " + createdLat;
												info "Created Lon: " + createdLon;
												// Fetch all sites from Sites_Reference
												allSites = Sites_Reference[Latitude1 != null];
												// Debug: Check how many sites were found
												info "Number of sites found: " + allSites.count();
												// Initialize variables for tracking closest site
												closestSite = null;
												minDistance = 999999999;
												// Calculate distance using Haversine formula
												for each  site in allSites
												{
													siteLat = site.Latitude1;
													siteLon = site.Longitude1;
													// Skip sites without coordinates
													if(siteLat != null && siteLon != null)
													{
														// Debug: Check site coordinates
														info "Site: " + site.Site_Name + " - Lat: " + siteLat + ", Lon: " + siteLon;
														// Haversine formula - distance in kilometers
														R = 6371;
														// Convert degrees to radians
														lat1Rad = (createdLat * 3.14159265359) / 180;
														lat2Rad = (siteLat * 3.14159265359) / 180;
														deltaLat = ((siteLat - createdLat) * 3.14159265359) / 180;
														deltaLon = ((siteLon - createdLon) * 3.14159265359) / 180;
														// Haversine calculation
														a = sin(deltaLat / 2) * sin(deltaLat / 2) + cos(lat1Rad) * cos(lat2Rad) * sin(deltaLon / 2) * sin(deltaLon / 2);
														c = 2 * atan2(sqrt(a),sqrt(1 - a));
														distance = R * c;
														// Debug: Check calculated distance
														info "Distance to " + site.Site_Name + ": " + distance + " km";
														// Check if this is the closest site
														if(distance < minDistance)
														{
															minDistance = distance;
															closestSite = site;
															info "New closest site: " + site.Site_Name;
														}
													}
												}
												// Update the RAMS record with closest site information
												if(closestSite != null)
												{
													info "Final closest site: " + closestSite.get("Site_Name") + " at " + minDistance.round(2) + " km";
													input.Closest_Site = closestSite.ID;
													input.Distance_to_Site = minDistance.round(2);
												}
												else
												{
													info "ERROR: No closest site found after checking all sites!";
												}
											}
											catch (e)
											{
												info "ERROR parsing location: " + e;
											}
										}
										else
										{
											info "ERROR: No location data available";
										}
						)
					}
				}

