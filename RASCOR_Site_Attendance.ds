											dylanEmail = "dylan.byrne12322@gmail.com";
											shaneEmail = "u3094959461@gmail.com";
											isDylanOrShane = userEmail == dylanEmail || userEmail == shaneEmail;
											// ============================================
											// CHECK IF EMPLOYEE EXISTS
											// ============================================
											existingEmployee = Employees_Reference[System_Email_Address == userEmail];
											if(existingEmployee.count() > 0)
											{
												info "Found existing employee with email: " + userEmail;
												for each  emp in existingEmployee
												{
													oldDeviceId = emp.GeoTrackerID;
