				 float calculateDistance(float lat1, float lon1, float lat2, float lon2)
                 {
                 	// Haversine formula
                 	R = 6371000;
                 	// Earth's radius in meters
                 	lat1Rad = lat1 * 3.14159265359 / 180.0;
                 	lat2Rad = lat2 * 3.14159265359 / 180.0;
                 	deltaLat = (lat2 - lat1) * 3.14159265359 / 180.0;
                 	deltaLon = (lon2 - lon1) * 3.14159265359 / 180.0;
                 	a = sin(deltaLat / 2) * sin(deltaLat / 2) + cos(lat1Rad) * cos(lat2Rad) * sin(deltaLon / 2) * sin(deltaLon / 2);
                 	c = 2 * atan2(sqrt(a),sqrt(1 - a));
                 	distance = R * c;
                 	return distance;
                 }
