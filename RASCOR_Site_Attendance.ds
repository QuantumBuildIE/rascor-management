										{
											info "No email found - skipping push notification";
											thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"No email found - skipping push notification","WARNING");
										}
										// ====================================================================
										// NOTIFICATION 3: Email (Backup - has working link)
										// ====================================================================
										if(userEmail != null && userEmail != "")
