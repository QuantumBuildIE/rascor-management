												info "âœ“ Push notification sent: " + pushResult;
												thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"Push notification sent: " + pushResult,"INFO");
											}
											catch (e)
											{
												info "âš ï¸ Push notification failed: " + e;
												thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"Push notification failed: " + e,"ERROR");
											}
