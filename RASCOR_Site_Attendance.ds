			Check_SPA_and_Notify as "Check_SPA_and_Notify"
			{
				type =  form
				form = Sample_Activity_Data_Report1
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											// ====================================================================
										// SPA NOTIFICATION WORKFLOW - SMS + PUSH + EMAIL VERSION
										// Sends SMS, Push Notification, and Email if first site entry has no SPA
										// ====================================================================
										info "=== SPA Check Workflow Started ===";
										info "Event Type: " + input.Event_Type1;
										info "User ID: " + input.User_ID;
										info "Site ID: " + input.Site_ID;
										info "Timestamp: " + input.Timestamp;
										// Only process Enter events
										if(input.Event_Type1 != "Enter")
										{
											info "Not an Enter event - skipping SPA check";
											return;
										}
										thisapp.logDebug("Check_SPA_and_Notify",input.User_ID,input.Site_ID,input.ID.toString(),"=== SPA Check Workflow Started ===","INFO");
										info "âœ“ This is an Enter event - proceeding with SPA check";
										// Get event details
										userId = input.User_ID;
										siteId = input.Site_ID;
										siteName = input.Site_Name.toString();
										eventTimestamp = input.Timestamp;
										eventDate = eventTimestamp.toDate();
										info "Processing: User=" + userId + ", Site=" + siteId + ", Date=" + eventDate;
										// Check if this is the FIRST Enter event of the day for this user/site
										todayStart = eventDate.toString("yyyy-MM-dd") + " 00:00:00";
										todayEnd = eventDate.toString("yyyy-MM-dd") + " 23:59:59";
										info "Checking for previous entries today between " + todayStart + " and " + todayEnd;
										thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"Checking for previous entries today between " + todayStart + " and " + todayEnd,"INFO");
										// Query for other Enter events today for this user/site (excluding current record)
										userIdStr = userId.toString();
										siteIdStr = siteId.toString();
										previousEntersToday = Sample_Activity_Data_Report1[User_ID == userIdStr && Site_ID == siteIdStr && Event_Type1 == "Enter" && Timestamp >= todayStart.toTime() && Timestamp <= todayEnd.toTime() && ID != input.ID];
										enterCount = previousEntersToday.count();
										info "Found " + enterCount + " previous Enter events today";
										if(enterCount > 0)
										{
											info "Not the first Enter event today - no notification needed";
											return;
										}
										info "âœ“ This is the FIRST Enter event today - checking for SPA";
										thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"This is the FIRST Enter event today - checking for SPA","INFO");
										// Check for SPA within 5-minute grace period
										gracePeriodMinutes = 5;
										checkTimeStart = eventTimestamp.subMinutes(gracePeriodMinutes);
										checkTimeEnd = eventTimestamp.addMinutes(gracePeriodMinutes);
										info "Checking for SPA between: " + checkTimeStart + " and " + checkTimeEnd;
										thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"Checking for SPA between: " + checkTimeStart + " and " + checkTimeEnd,"INFO");
										// Search for existing SPA record using Added_Time
										existingSPA = SPA[User_ID == userIdStr && Site_ID == siteIdStr && Added_Time >= checkTimeStart && Added_Time <= checkTimeEnd];
										spaCount = existingSPA.count();
										info "Found " + spaCount + " existing SPA records in grace period";
										if(spaCount > 0)
										{
											info "âœ“ SPA exists - no notification needed";
											return;
										}
										info "âš ï¸ No SPA found - sending notifications";
										thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"No SPA found - sending notifications","INFO");
										// Get user phone and email from Employees_Reference_Report
										userPhone = null;
										userEmail = null;
										info "Looking up contact info for User_ID: " + userId;
										try 
										{
											// Use zoho.creator.getRecords API
											criteria = "GeoTrackerID == \"" + userId + "\"";
											info "Criteria used to find contact info for User_ID: " + criteria;
											employeeData = zoho.creator.getRecords("quantumbuild1","rascor-site-attendance","Employees_Reference_Report",criteria,1,1,"creator_connection");
											info "Query response: " + employeeData;
											if(employeeData != null && employeeData.get("code") == 3000)
											{
												dataList = employeeData.get("data");
												info "DataLis records returned: " + dataList.size();
												if(dataList != null && dataList.size() > 0)
												{
													userRecord = dataList.get(0);
													userPhone = userRecord.get("Phone_Number");
													userEmail = userRecord.get("System_Email_Address");
													info "Found user - Phone: " + userPhone + ", Email: " + userEmail;
													thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"Found user - Phone: " + userPhone + ", Email: " + userEmail,"INFO");
												}
												else
												{
													info "WARNING: No employee record found for User_ID: " + userId;
													info "Continuing with null contact info - notifications will be skipped";
													thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"WARNING: No employee record found for User_ID: " + userId + ". Continuing with null contact info - notifications will be skipped","WARNING");
												}
											}
											else
											{
												info "ERROR: API call failed or returned error code";
												info "Response: " + employeeData;
												thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"ERROR: API call failed or returned error code. Response: " + employeeData,"ERROR");
											}
										}
										catch (e)
										{
											info "ERROR: Exception during employee lookup: " + e;
											thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"ERROR: Exception during employee lookup: " + e,"ERROR");
										}
										// Continue even if contact info not found - just won't send notifications
										if(userPhone == null && userEmail == null)
										{
											info "âš ï¸ No contact information available - cannot send notifications";
											info "Please ensure User_ID " + userId + " has phone/email in Employees_Reference";
											thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"No contact information available - cannot send notifications. Please ensure User_ID " + userId + " has phone/email in Employees_Reference","INFO");
											return;
											// Exit workflow gracefully
										}
										// Build form URL with pre-populated fields
										formURL = "https://creator.zoho.eu/quantumbuild1/rascor-site-attendance#Form:SPA?User_ID=" + userId + "&Site_ID=" + siteId + "&Event_Date=" + eventDate.toString("dd-MMM-yyyy");
										info "Form URL: " + formURL;
										thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"Form URL: " + formURL,"INFO");
										// Use site name if available, otherwise use site ID
										siteDisplayName = siteName;
										if(siteDisplayName == null || siteDisplayName == "")
										{
											siteDisplayName = siteId;
										}
										// ====================================================================
										// NOTIFICATION 1: SMS (Primary - has working link)
										// ====================================================================
										if(userPhone != null && userPhone != "")
										{
											// Build SMS message (keep concise for standard SMS)
											smsMessage = "SPA Required: You entered " + siteDisplayName + ". Submit SPA: " + formURL;
											info "Sending SMS to: " + userPhone;
											info "SMS message: " + smsMessage;
											info "SMS length: " + smsMessage.length() + " characters";
											try 
											{
												// Send SMS via Twilio
												//smsResult = twilio.sms.send("twilio_sms_connection",userPhone.toString(),smsMessage.toString());
												//info "âœ“ SMS sent successfully: " + smsResult;
												//thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"SMS sent successfully: " + smsResult,"INFO");
												info "SMS Sending has been disabled in workflow";
												thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"WARNING: SMS Sending has been disabled in the 'Check_SPA_and_notify' workflow, code has been commented out for now","INFO");
											}
											catch (e)
											{
												info "âš ï¸ SMS send failed: " + e;
												thisapp.logDebug("Check_SPA_and_Notify",userId,siteId,input.ID.toString(),"SMS send failed: " + e,"ERROR");
											}
										}
										else
										{
											info "No phone number found - skipping SMS";
										}
										// ====================================================================
										// NOTIFICATION 2: Push Notification (Alert only - no link)
										// ====================================================================
										if(userEmail != null && userEmail != "")
										{
											try 
											{
												info "Sending push notification to: " + userEmail;
												// Prepare notification content
												pushTitle = "SPA Required";
												pushMessage = "You entered " + siteDisplayName + " but no SPA was recorded. Please submit your SPA.";
