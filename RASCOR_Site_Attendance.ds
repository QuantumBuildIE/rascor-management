		 functions
		 {
			 Deluge
			 {
				 float A01_calculateDistance(float lat1, float lon1, float lat2, float lon2)
                 {
                 	// Haversine formula
                 	R = 6371000;
                 	// Earth's radius in meters
                 	lat1Rad = lat1 * 3.14159265359 / 180.0;
                 	lat2Rad = lat2 * 3.14159265359 / 180.0;
                 	deltaLat = (lat2 - lat1) * 3.14159265359 / 180.0;
                 	deltaLon = (lon2 - lon1) * 3.14159265359 / 180.0;
                 	a = sin(deltaLat / 2) * sin(deltaLat / 2) + cos(lat1Rad) * cos(lat2Rad) * sin(deltaLon / 2) * sin(deltaLon / 2);
                 	c = 2 * atan2(sqrt(a),sqrt(1 - a));
                 	distance = R * c;
                 	return distance;
                 }
				 void A01_Calculate_Time_OnSite()
                 {
                 	msg = "A01_Calculate_Time_OnSite";
                 	// Will always be yesterday as will be run at 01:00 am	
                 	// Start of yesterday (00:00:00)
                 	start_Yesterday = zoho.currentdate.addDay(-1);
                 	end_Date = zoho.currentdate;
                 	// will just be uo to 01:00 toiday so thats fine	
                 	allRecords = Sample_Activity_Data_Report1[ID != 0 && Ignore_Noise == false || Ignore_Noise == null && Added_Time >= start_Yesterday && Added_Time <= end_Date];
                 	employeeMap = Map();
                 	for each  rec in allRecords
                 	{
                 		employeeName = rec.Employee_Name_Text;
                 		addedTime = rec.Added_Time;
                 		eventType = rec.Event_Type1;
                 		recordID = rec.ID;
                 		siteID = rec.Site_ID;
                 		dateOnly = addedTime.toDate().toString("dd-MMM-yyyy");
                 		key = employeeName + "_" + dateOnly + "_" + siteID;
                 		if(employeeMap.contains(key) == false)
                 		{
                 			newList = List();
                 			employeeMap.put(key,newList);
                 		}
                 		recList = employeeMap.get(key);
                 		newRec = Map();
                 		newRec.put("ID",recordID);
                 		newRec.put("Added_Time",addedTime);
                 		newRec.put("Event_Type",eventType);
                 		recList.add(newRec);
                 	}
                 	for each  key in employeeMap.keys()
                 	{
                 		recordList = employeeMap.get(key);
                 		entryRecord = null;
                 		lastExitTime = null;
                 		for each  rec in recordList
                 		{
                 			eventType = rec.get("Event_Type");
                 			currentTime = rec.get("Added_Time");
                 			currentID = rec.get("ID");
                 			if(eventType == "Enter")
                 			{
                 				// If we have a pending entry with an exit, calculate and save it
                 				if(entryRecord != null && lastExitTime != null)
                 				{
                 					entryTime = entryRecord.get("Added_Time");
                 					diffMillis = lastExitTime.toTime() - entryTime.toTime();
                 					timeOnSiteMinutesDecimal = (diffMillis / (1000.0 * 60.0)).round(2);
                 					entryRecID = entryRecord.get("ID");
                 					// Update only this entry record with its session time
                 					for each  updateRec in Sample_Activity_Data_Report1[ID == entryRecID]
                 					{
                 						updateRec.TimeOnSiteInMinutesDecimal=timeOnSiteMinutesDecimal;
                 					}
                 				}
                 				// Start tracking this new entry
                 				entryRecord = rec;
                 				lastExitTime = null;
                 			}
                 			else if(eventType == "Exit")
                 			{
                 				lastExitTime = currentTime;
                 			}
                 		}
                 		// Handle the last entry-exit pair for this day
                 		if(entryRecord != null && lastExitTime != null)
                 		{
                 			entryTime = entryRecord.get("Added_Time");
                 			diffMillis = lastExitTime.toTime() - entryTime.toTime();
                 			timeOnSiteMinutesDecimal = (diffMillis / (1000.0 * 60.0)).round(2);
                 			entryRecID = entryRecord.get("ID");
                 			// Update only this entry record with its session time
                 			for each  updateRec in Sample_Activity_Data_Report1[ID == entryRecID]
                 			{
                 				updateRec.TimeOnSiteInMinutesDecimal=timeOnSiteMinutesDecimal;
                 			}
                 		}
                 	}
                 	sendmail
                 	[
                 		from :zoho.adminuserid
                 		to :zoho.adminuserid
                 		subject :"A01_Calculate_Time_OnSite"
                 		message :"<div>" + msg + "n&nbsp;<br></div>"
                 	]
                 }
				 void A01_Filter_Noise_Records()
                 {
                 	allRecords = Sample_Activity_Data_Report1[ID != 0];
                 	info "Starting noise filtering...";
                 	noiseThresholdMeters = 150;
                 	for each  rec in allRecords
                 	{
                 		employeeName = rec.Employee_Name_Text;
                 		siteName = rec.Site_Name_Text;
                 		addedTime = rec.Added_Time;
                 		eventType = rec.Event_Type1;
                 		recordID = rec.ID.toLong();
                 		dateOnly = addedTime.toDate();
                 		latitude = rec.Latitude1;
                 		longitude = rec.Longitude1;
                 		isNoise = false;
                 		if(eventType == "Enter")
                 		{
                 			firstEntryFound = false;
                 			firstLat = null;
                 			firstLon = null;
                 			// Search for earlier entries of this employee-site on the same day
                 			allDayRecords = Sample_Activity_Data_Report1[Employee_Name_Text == employeeName && Site_Name_Text == siteName && Event_Type1 == "Enter"];
                 			for each  checkRec in allDayRecords
                 			{
                 				checkDate = checkRec.Added_Time.toDate();
                 				checkID = checkRec.ID.toLong();
                 				if(checkDate == dateOnly && checkID < recordID)
                 				{
                 					if(firstEntryFound == false)
                 					{
                 						firstLat = checkRec.Latitude1;
                 						firstLon = checkRec.Longitude1;
                 						firstEntryFound = true;
                 					}
                 				}
                 			}
                 			if(firstEntryFound && firstLat != null && firstLon != null && latitude != null && longitude != null)
                 			{
                 				latDiff = latitude.toDecimal() - firstLat.toDecimal();
                 				lonDiff = longitude.toDecimal() - firstLon.toDecimal();
                 				latDiffSq = latDiff * latDiff;
                 				lonDiffSq = lonDiff * lonDiff;
                 				approxDistance = sqrt(latDiffSq + lonDiffSq) * 111000;
                 				if(approxDistance < noiseThresholdMeters)
                 				{
                 					isNoise = true;
                 					info "Noise detected: " + employeeName + " at " + siteName + " on " + dateOnly.toString("dd-MMM-yyyy") + " - Approx distance: " + approxDistance.round(1) + "m";
                 				}
                 			}
                 		}
                 		else if(eventType == "Exit")
                 		{
                 			// Check distance from first entry of the day
                 			allDayRecords = Sample_Activity_Data_Report1[Employee_Name_Text == employeeName && Site_Name_Text == siteName && Event_Type1 == "Enter"];
                 			firstLat = null;
                 			firstLon = null;
                 			foundFirst = false;
                 			for each  checkRec in allDayRecords
                 			{
                 				checkDate = checkRec.Added_Time.toDate();
                 				if(checkDate == dateOnly && foundFirst == false)
                 				{
                 					firstLat = checkRec.Latitude1;
                 					firstLon = checkRec.Longitude1;
                 					foundFirst = true;
                 				}
                 			}
                 			if(firstLat != null && firstLon != null && latitude != null && longitude != null)
                 			{
                 				latDiff = latitude.toDecimal() - firstLat.toDecimal();
                 				lonDiff = longitude.toDecimal() - firstLon.toDecimal();
                 				latDiffSq = latDiff * latDiff;
                 				lonDiffSq = lonDiff * lonDiff;
                 				approxDistance = sqrt(latDiffSq + lonDiffSq) * 111000;
                 				if(approxDistance < noiseThresholdMeters)
                 				{
                 					isNoise = true;
                 					info "Noise detected: " + employeeName + " exit at " + siteName + " on " + dateOnly.toString("dd-MMM-yyyy") + " - Approx distance: " + approxDistance.round(1) + "m";
                 				}
                 			}
                 		}
                 		rec.Ignore_Noise=isNoise;
                 		rec.Noise_Distance=approxDistance;
                 	}
                 	info "Noise filtering complete";
                 }
				 string A01_Generate_TimeOnSite_Analysis()
                 {
                 	param = Parameters[ID != 0];
                 	Report_Day = zoho.currentdate.subDay(1);
                 	startOfDay = Report_Day.toDateTime("00:00:00");
                 	endOfDay = Report_Day.addHour(23);
                 	info startOfDay;
                 	info endOfDay;
                 	// Update this query to exclude noise:
                 	allRecords = Sample_Activity_Data_Report1[Event_Type1 == "Enter" && TimeOnSiteInMinutesDecimal != null && Timestamp >= startOfDay && Timestamp <= endOfDay && Ignore_Noise == false || Ignore_Noise == null];
                 	info "Records fetched: Processing time on site data";
                 	// Expected hours per day
                 	expectedHoursPerDay = 7.5;
                 	// Find date range from the data
                 	minDate = null;
                 	maxDate = null;
                 	for each  rec in allRecords
                 	{
                 		recDate = rec.Added_Time.toDate();
                 		if(minDate == null || recDate < minDate)
                 		{
                 			minDate = recDate;
                 		}
                 		if(maxDate == null || recDate > maxDate)
                 		{
                 			maxDate = recDate;
                 		}
                 	}
                 	// Build aggregated summaries
                 	employeeMap = Map();
                 	siteMap = Map();
                 	dateMap = Map();
                 	siteEmployeeMap = Map();
                 	totalHours = 0.0;
                 	recordCount = 0;
                 	// Aggregate data - now processing ALL entry records (no first-entry filtering)
                 	for each  rec in allRecords
                 	{
                 		employeeName = rec.Employee_Name_Text.toString();
                 		siteName = rec.Site_Name_Text.toString();
                 		dateAdded = rec.Added_Time.toDate().toString("dd-MMM-yyyy");
                 		timeMinutes = rec.TimeOnSiteInMinutesDecimal;
                 		timeHours = (timeMinutes / 60.0).round(2);
                 		// Employee totals
                 		if(employeeMap.containKey(employeeName))
                 		{
                 			employeeMap.put(employeeName,employeeMap.get(employeeName) + timeHours);
                 		}
                 		else
                 		{
                 			employeeMap.put(employeeName,timeHours);
