				 void A01_CreateScheduleTalks()
                 {
                 	Counter = 0;
                 	// Get the talks to be scheduled foer today as this wiill run after midnight 2:00 AM
                 	for each  To_Be_Scheduled_Tasks in Schedule_Toobox_Talks[ID != 0 && All_Talks_Scheduled == false]
                 	{
                 		To_Be_Scheduled_Tasks.All_Talks_Scheduled=true;
                 		// Get the individual tasks for each sectron 1 - 7
                 		for each  To_Be_Scheduled_Tasks_Task_Row in To_Be_Scheduled_Tasks.Talks_To_be_Scheduled
                 		{
                 			Operator = Operators[ID == To_Be_Scheduled_Tasks_Task_Row.Operator];
                 			To_Be_Scheduled_Tasks_Task_Row.Talk_Scheduled=true;
                 			// Get the talk
                 			Talk = Toolbox_Talks[ID == To_Be_Scheduled_Tasks.Toolbox_Talk];
                 			// Get each section to be bult into the schedule 1-7
                 			for each  Talk_Row in Talk.Instructions
                 			{
                 				Counter = Counter + 1;
                 				// Parse wach talk section from the talk items 
                 				// Translate text
                 				Translated_Text = thisapp.A01_Translate_Text(Talk_Row.Instruction,Operator.Language);
                 				if(Counter = 1)
                 				{
                 					Section1 = Translated_Text;
                 				}
                 				if(Counter = 2)
                 				{
                 					Section2 = Translated_Text;
                 				}
                 				if(Counter = 3)
                 				{
                 					Section3 = Translated_Text;
                 				}
                 				if(Counter = 4)
                 				{
                 					Section4 = Translated_Text;
                 				}
                 				if(Counter = 5)
                 				{
                 					Section5 = Translated_Text;
                 				}
                 				if(Counter = 6)
                 				{
                 					Section6 = Translated_Text;
                 				}
                 				if(Counter = 7)
                 				{
                 					Section7 = Translated_Text;
                 				}
                 			}
                 			// We have the talk and each instruction , now we need to create teh aqctual schedule
                 			var = insert into Scheduled_Talks
                 			[
                 				Added_User=zoho.loginuser
                 				Toolbox_Talks=Talk.ID
                 				File_upload=Talk.File_upload
                 				Operator=To_Be_Scheduled_Tasks_Task_Row.Operator
                 				Required_Date=To_Be_Scheduled_Tasks.Schedule_For_Date
                 				Section_11=Section1
                 				Section_1_Read=false
                 				Section_21=Section2
                 				Section_2_Read=False
                 				Section_31=Section3
                 				Section_3_Read=false
                 				Section_41=Section4
                 				Section_4_Read=false
                 				Section_51=Section5
                 				Section_5_Read=false
                 				Section_61=Section6
                 				Section_6_Read=false
                 				Section_71=Section7
                 				Section_7_Read=false
                 			];
                 			videoURL = thisapp.A14_GetVideoURLForOperator(Talk.ID,Operator.ID);
                 			url = "https://dosca61.github.io/toolbox-video/?video=" + encodeUrl(videoURL) + "&talkid=" + var;
                 			sub = thisapp.A01_Translate_Text(Operator.Language,"Toolbox Talk " + Talk.Talk + " Has been Scheduled For You");
