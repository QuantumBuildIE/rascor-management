				{
				condition = (Reminders_Sent >= 3)
				fields = [Reminders_Sent]
				format = "color:#ffffff;background-color:#e84c3d;"
				}
				"0 and Done"
				{
				condition = (Reminders_Sent == 0 && All_Sections_Read == true)
				fields = [Reminders_Sent]
				format = "color:#000000;background-color:#1bbc9b;"
				}
				"Outstanding less that 3"
				{
				condition = (Reminders_Sent < 3 && Reminders_Sent > 0 && All_Sections_Read == false)
				fields = [Reminders_Sent]
				format = "color:#333;background-color:#f1c40f;"
