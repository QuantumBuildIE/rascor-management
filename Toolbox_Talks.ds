			CreateTranslatedTalkifNec as "CreateTranslatedTalkifNecessary"
			{
				type =  form
				form = Operators
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											// A04_GenerateTranslationsForNewLanguage
										// Trigger: When new language is added to an Operator
										if(input.Translate_Now == true)
										{
											newLang = input.Language;
											// Check if this language already has translations
											existingTranslations = Translated_Talks[Language == newLang].count();
											if(existingTranslations == 0 && newLang != "English" && newLang != null)
											{
												// New language - generate translations for all talks
												allTalks = Toolbox_Talks[ID != null];
												for each  talk in allTalks
												{
													// Call existing translation function
													thisapp.A03_GenerateTranslations(talk.ID);
												}
												info "Generated translations for " + allTalks.count() + " talks in " + newLang;
											}
										}
						)
					}
				}

			}
