										{
											if(row.Correct_Answer != null && row.Correct_Answer != "")
											{
												row.Correct_Answer=row.Correct_Answer.toLowerCase().trim();
											}
											if(row.Answers != null && row.Answers != "")
											{
												row.Answers=row.Answers.toLowerCase().trim();
											}
											if(row.Question != null && row.Question != "")
											{
												row.Question=row.Question.toLowerCase().trim();
											}
										}
						)
					}
				}

