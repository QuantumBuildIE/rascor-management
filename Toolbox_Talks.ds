			{
				type =  form
				form = Zappier_Picked_Video
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											// Get values from the new Zapier record
										projectId = input.ElevenLabs_Project_ID;
										videoURL = input.Dubbed_Video_URL;
										info "Updating Translated_Videos for project: " + projectId;
										// Find and update the matching Translated_Videos record
										for each  video in Translated_Videos[ElevenLabs_Project_ID == projectId]
										{
											video.Dubbed_Video_URL=videoURL;
											video.Status="Complete";
											video.Completed_Time=zoho.currenttime;
											info "Updated record ID: " + video.ID;
										}
						)
					}
				}

