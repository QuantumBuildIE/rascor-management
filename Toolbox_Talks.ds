				 void A12_ProcessVideoDubbing()
                 {
                 	// ============================================================
                 	// A12_ProcessVideoDubbing
                 	// ============================================================
                 	// Purpose: Send pending videos to ElevenLabs for dubbing
                 	// Schedule: Run after A11, or every few hours
                 	// ============================================================
                 	info "Starting A12_ProcessVideoDubbing";
                 	// Get API key from Parameters
                 	params = Parameters[ID != null];
                 	apiKey = params.ElevenLabs_API_Key;
                 	if(apiKey == null || apiKey == "")
                 	{
                 		info "ERROR: ElevenLabs API key not found in Parameters";
                 		return;
                 	}
                 	// Process pending videos (limit to 5 at a time to avoid timeouts)
                 	pendingVideos = Translated_Videos[Status == "Pending"];
                 	counter = 0;
                 	for each  video in pendingVideos
                 	{
                 		counter = counter + 1;
                 		if(counter > 5)
                 		{
                 			break;
                 		}
                 		// rest of the code...
                 		info "Processing video ID: " + video.ID + " for language: " + video.Language;
                 		// Update status to Processing
                 		video.Status="Processing";
                 		// Get the original video URL
                 		originalURL = video.Original_Video_URL;
                 		// Convert Google Drive share link to direct download URL
                 		directURL = thisapp.A12_ConvertDriveURL(originalURL);
                 		if(directURL == "")
                 		{
                 			video.Status="Failed";
                 			video.Error_Message="Could not convert Drive URL to direct download link";
                 			continue;
                 		}
                 		// Map language to ElevenLabs language code
                 		targetLang = thisapp.A12_GetLanguageCode(video.Language);
                 		if(targetLang == "")
                 		{
                 			video.Status="Failed";
                 			video.Error_Message="Unsupported language: " + video.Language;
                 			continue;
                 		}
                 		// Call ElevenLabs Dubbing API
                 		try 
                 		{
                 			// Prepare the API request
                 			headers = Map();
                 			headers.put("xi-api-key",apiKey);
                 			// Create multipart form data for dubbing request
                 			formData = Map();
                 			formData.put("source_url",directURL);
                 			formData.put("target_lang",targetLang);
                 			formData.put("mode","automatic");
                 			formData.put("watermark",true);
                 			formData.put("num_speakers",0);
                 			// Auto-detect
                 			// Make API call
                 			response = invokeurl
                 			[
                 				url :"https://api.elevenlabs.io/v1/dubbing"
                 				type :POST
                 				parameters:formData
                 				headers:headers
                 			];
                 			info "ElevenLabs API Response: " + response;
                 			// Parse response
                 			if(response.contains("dubbing_id"))
                 			{
                 				responseMap = response.toMap();
                 				dubbingId = responseMap.get("dubbing_id");
                 				video.ElevenLabs_Project_ID=dubbingId;
                 				video.Status="Processing";
                 				video.Error_Message="";
                 				info "Dubbing started - ID: " + dubbingId;
                 			}
                 			else
                 			{
                 				video.Status="Failed";
                 				video.Error_Message="API Error: " + response;
                 				info "Dubbing failed: " + response;
                 			}
                 		}
                 		catch (e)
                 		{
                 			video.Status="Failed";
                 			video.Error_Message="Exception: " + e.toString();
                 			info "Exception during dubbing: " + e;
                 		}
                 	}
                 	info "A12_ProcessVideoDubbing complete";
                 }
				 void A13_PollDubbingStatus()
                 {
                 	info "Starting A13_PollDubbingStatus";
                 	params = Parameters[ID != null];
                 	apiKey = params.ElevenLabs_API_Key;
                 	if(apiKey == null || apiKey == "")
                 	{
                 		info "ERROR: ElevenLabs API key not found in Parameters";
                 		return;
                 	}
                 	// Collect ready videos for email summary
                 	readyList = List();
                 	// Get all videos currently processing
                 	processingVideos = Translated_Videos[Status == "Processing" && ElevenLabs_Project_ID != null && ElevenLabs_Project_ID != ""];
                 	for each  video in processingVideos
                 	{
                 		dubbingId = video.ElevenLabs_Project_ID;
                 		info "Checking status for dubbing ID: " + dubbingId;
                 		try 
                 		{
                 			headers = Map();
                 			headers.put("xi-api-key",apiKey);
                 			statusResponse = invokeurl
                 			[
                 				url :"https://api.elevenlabs.io/v1/dubbing/" + dubbingId
                 				type :GET
                 				headers:headers
                 			];
                 			info "Status response: " + statusResponse;
                 			statusMap = statusResponse.toMap();
                 			dubbingStatus = statusMap.get("status");
                 			if(dubbingStatus == "dubbed")
                 			{
                 				// Get talk name
                 				talk = Toolbox_Talks[ID == video.Toolbox_Talk];
                 				talkName = ifnull(talk.Talk,"Video");
                 				// Build suggested filename
                 				cleanName = talkName.replaceAll("[^a-zA-Z0-9]","_");
                 				suggestedFileName = cleanName + "_" + video.Language + "_dubbed.mp4";
                 				// Get language code for download URL
                 				targetLang = thisapp.A12_GetLanguageCode(video.Language);
                 				downloadUrl = "https://api.elevenlabs.io/v1/dubbing/" + dubbingId + "/audio/" + targetLang;
                 				video.Status="Ready_For_Download";
                 				video.Error_Message="Save as: " + suggestedFileName + " | URL: " + downloadUrl;
                 				info "Ready for download: " + suggestedFileName;
                 				// Add to email list
                 				readyList.add({"talk":talkName,"language":video.Language,"filename":suggestedFileName,"url":downloadUrl});
                 			}
                 			else if(dubbingStatus == "failed")
                 			{
                 				errorMsg = ifnull(statusMap.get("error"),"Unknown error");
                 				video.Status="Failed";
                 				video.Error_Message="Dubbing failed: " + errorMsg;
                 				info "Dubbing failed for ID: " + dubbingId + " - " + errorMsg;
                 			}
                 			else
                 			{
                 				info "Still processing: " + dubbingId + " - Status: " + dubbingStatus;
                 			}
                 		}
                 		catch (e)
                 		{
                 			info "Exception checking status for " + dubbingId + ": " + e;
                 		}
                 	}
                 	// Send email if any videos are ready
                 	if(readyList.size() > 0)
                 	{
                 		emailBody = "<h2>Video Translations Ready for Download</h2>";
                 		emailBody = emailBody + "<p>" + readyList.size() + " video(s) are ready:</p>";
                 		emailBody = emailBody + "<table border='1' cellpadding='8' cellspacing='0'>";
                 		emailBody = emailBody + "<tr><th>Talk</th><th>Language</th><th>Save As</th><th>Download</th></tr>";
                 		for each  item in readyList
                 		{
                 			emailBody = emailBody + "<tr>";
                 			emailBody = emailBody + "<td>" + item.get("talk") + "</td>";
                 			emailBody = emailBody + "<td>" + item.get("language") + "</td>";
                 			emailBody = emailBody + "<td>" + item.get("filename") + "</td>";
                 			emailBody = emailBody + "<td><a href='" + item.get("url") + "'>Download</a></td>";
                 			emailBody = emailBody + "</tr>";
                 		}
                 		emailBody = emailBody + "</table>";
                 		emailBody = emailBody + "<p><strong>Note:</strong> You'll need to add the API key header (xi-api-key) to download. Use a tool like Postman or the ElevenLabs dashboard.</p>";
                 		sendmail
                 		[
                 			from :zoho.loginuserid
                 			to :zoho.loginuserid
                 			subject :"Video Translations Ready - " + readyList.size() + " video(s)"
                 			message :emailBody
                 		]
                 		info "Email sent for " + readyList.size() + " ready videos";
                 	}
                 	info "A13_PollDubbingStatus complete";
                 }
				 string A13_UploadToGoogleDrive(string downloadURL, int videoRecordID, string apiKey)
                 {
                 	shareURL = "";
                 	try 
                 	{
                 		// Get the video record
                 		videoRecord = Translated_Videos[ID == videoRecordID];
                 		// Get the talk name for the filename
                 		talk = Toolbox_Talks[ID == videoRecord.Toolbox_Talk];
                 		talkName = ifnull(talk.Talk,"Video");
                 		language = videoRecord.Language;
                 		// Clean filename
                 		fileName = talkName.replaceAll("[^a-zA-Z0-9]","_") + "_" + language + "_dubbed.mp4";
                 		// Download the file from ElevenLabs
                 		dubbedFile = invokeurl
                 		[
                 			url :downloadURL
                 			type :GET
                 			headers:{"xi-api-key":apiKey}
                 			response-format:FILE
                 		];
                 		// Check if file downloaded
                 		if(dubbedFile == null)
                 		{
                 			info "Download failed - file is null";
                 			return "";
                 		}
                 		// Upload to Zoho WorkDrive
                 		folderId = "ftx9q5a970de85b754badb3c79263b580eb83";
                 		uploadResponse = zoho.workdrive.uploadFile(dubbedFile,folderId,fileName,true,"zoho_workdrive_connection");
                 		info "WorkDrive upload response: " + uploadResponse;
                 		if(uploadResponse.containKey("data"))
                 		{
                 			fileData = uploadResponse.get("data").get(0).get("attributes");
                 			permalink = fileData.get("Permalink");
                 			shareURL = permalink;
                 			info "File uploaded: " + shareURL;
                 		}
                 	}
                 	catch (e)
                 	{
                 		info "Exception uploading to WorkDrive: " + e;
                 	}
                 	return shareURL;
                 }
