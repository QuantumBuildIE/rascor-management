				 void A07_SendReminders()
                 {
                 	// ===========================================
                 	// SEND REMINDERS FOR UNREAD SCHEDULED TALKS
                 	// ===========================================
                 	todayDate = zoho.currentdate;
                 	for each  scheduledTalk in Scheduled_Talks[All_Sections_Read == false && Required_Date < todayDate]
                 	{
                 		// Get operator details
                 		Operator = Operators[ID == scheduledTalk.Operator];
                 		if(Operator == null)
                 		{
                 			continue;
                 		}
                 		// Get talk details
                 		Talk = Toolbox_Talks[ID == scheduledTalk.Toolbox_Talks];
                 		if(Talk == null)
                 		{
                 			continue;
                 		}
                 		// Increment reminders sent
                 		currentReminders = ifnull(scheduledTalk.Reminders_Sent,0);
                 		scheduledTalk.Reminders_Sent=currentReminders + 1;
                 		// Build URL
                 		videoURL = thisapp.A14_GetVideoURLForOperator(Talk.ID,Operator.ID);
                 		url = "https://dosca61.github.io/toolbox-video/?video=" + encodeUrl(videoURL) + "&talkid=" + scheduledTalk.ID;
                 		// Get cached translation for email
                 		translation = thisapp.A04_GetCachedTranslation(Talk.ID,Operator.Language);
                 		if(translation.size() == 0)
                 		{
                 			translation = thisapp.A04_GetCachedTranslation(Talk.ID,"English");
                 		}
                 		// Build subject based on reminder count
                 		baseSubject = ifnull(translation.get("Email_Subject"),"Toolbox Talk " + Talk.Talk + " Has been Scheduled For You");
                 		if(currentReminders >= 3)
                 		{
                 			sub = "4th Reminder: " + baseSubject;
                 		}
                 		else
                 		{
                 			sub = "Reminded: " + baseSubject;
                 		}
                 		msg = ifnull(translation.get("Email_Message"),"A toolbox talk has been scheduled for you. Select the link to access your talks.");
                 		// Determine CC recipients
                 		ccList = "admin@richmondit.ie";
                 		if(currentReminders >= 3)
                 		{
                 			// Get Assigned PM email
                 			AssignedPM = Operators[ID == Operator.Assigned_PM];
                 			if(AssignedPM != null && AssignedPM.Email_Address != null)
                 			{
                 				ccList = ccList + "," + AssignedPM.Email_Address;
                 			}
                 		}
                 		// Send reminder email
                 		sendmail
                 		[
                 			from :zoho.loginuserid
                 			to :Operator.Email_Address
                 			cc:ccList
                 			subject :sub
                 			message :msg + "  " + url + "&nbsp;</div>"
                 		]
                 		info "Reminder " + scheduledTalk.Reminders_Sent + " sent to " + Operator.Email_Address + " for: " + Talk.Talk;
                 	}
                 	info "Reminder check complete";
                 }
