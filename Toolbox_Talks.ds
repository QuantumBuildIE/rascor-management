				 string A12_ConvertDriveURL(string shareURL)
                 {
                 	directURL = "";
                 	if(shareURL.contains("/file/d/"))
                 	{
                 		startIndex = shareURL.indexOf("/file/d/") + 8;
                 		endIndex = shareURL.indexOf("/",startIndex);
                 		if(endIndex == -1)
                 		{
                 			endIndex = shareURL.length();
                 		}
                 		fileId = shareURL.subString(startIndex,endIndex);
                 		directURL = "https://drive.google.com/uc?export=download&id=" + fileId;
                 	}
                 	else if(shareURL.contains("id="))
                 	{
                 		startIndex = shareURL.indexOf("id=") + 3;
                 		endIndex = shareURL.indexOf("&",startIndex);
                 		if(endIndex == -1)
                 		{
                 			endIndex = shareURL.length();
                 		}
                 		fileId = shareURL.subString(startIndex,endIndex);
                 		directURL = "https://drive.google.com/uc?export=download&id=" + fileId;
                 	}
                 	return directURL;
                 }
