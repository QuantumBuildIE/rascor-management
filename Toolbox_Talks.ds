				 string A01_Translate_Text(string targetLang, string sourceText)
                 {
                 	if(targetLang == "English" || sourceText == null || sourceText == "")
                 	{
                 		return sourceText;
                 	}
                 	param = Parameters[ID > 0];
                 	if(param == null)
                 	{
                 		info "No parameter record found";
                 		return sourceText;
                 	}
                 	ApiKey = param.ClaudeAPI;
                 	apiUrl = "https://api.anthropic.com/v1/messages";
                 	header = Map();
                 	header.put("x-api-key",ApiKey);
                 	header.put("anthropic-version","2023-06-01");
                 	header.put("content-type","application/json");
                 	prompt = "Translate the following text to " + targetLang + ". Only return the translated text, nothing else:\n\n" + sourceText;
                 	messageMap = Map();
                 	messageMap.put("role","user");
                 	messageMap.put("content",prompt);
                 	messages = List();
                 	messages.add(messageMap);
                 	body = Map();
                 	body.put("model","claude-sonnet-4-20250514");
                 	body.put("max_tokens",1024);
                 	body.put("messages",messages);
                 	response = invokeurl
                 	[
                 		url :apiUrl
                 		type :POST
                 		parameters:body.toString()
                 		headers:header
                 	];
                 	info "Translating to " + targetLang + ": " + response;
                 	contentList = response.getJSON("content");
                 	firstContent = contentList.get(0);
                 	translatedText = firstContent.getJSON("text");
                 	return translatedText;
                 }
				 void A02_ProcessNextTalk()
                 {
                 	// ===========================================
                 	// CONFIGURATION
                 	// ===========================================
                 	todayDate = zoho.currentdate;
                 	operatorCount = 0;
                 	maxOperators = 50;
                 	// ===========================================
                 	// MAIN LOOP: Process Schedule Records
                 	// ===========================================
                 	for each  rec in Schedule_Toobox_Talks[All_Talks_Scheduled == false && Schedule_For_Date <= todayDate]
                 	{
                 		Talk = Toolbox_Talks[ID == rec.Toolbox_Talk];
                 		allRowsProcessed = true;
                 		// ===========================================
                 		// INNER LOOP: Process Each Operator Row
                 		// ===========================================
                 		for each  taskRow in rec.Talks_To_be_Scheduled
                 		{
                 			// Skip already processed rows
                 			if(taskRow.Talk_Scheduled == true)
                 			{
                 				continue;
                 			}
                 			// Limit check
                 			if(operatorCount >= maxOperators)
                 			{
                 				allRowsProcessed = false;
                 				return;
                 			}
                 			// Get operator details
                 			Operator = Operators[ID == taskRow.Operator];
                 			taskRow.Talk_Scheduled=true;
                 			operatorCount = operatorCount + 1;
                 			// ===========================================
                 			// GET CACHED TRANSLATION (NO API CALLS!)
                 			// ===========================================
                 			translation = thisapp.A04_GetCachedTranslation(Talk.ID,Operator.Language);
                 			// Check if translation was found, fallback to English if empty
                 			if(translation.size() == 0)
                 			{
                 				translation = thisapp.A04_GetCachedTranslation(Talk.ID,"English");
                 			}
                 			// Extract sections from cached translation
                 			Section1 = ifnull(translation.get("Section_1"),"");
                 			Section2 = ifnull(translation.get("Section_2"),"");
                 			Section3 = ifnull(translation.get("Section_3"),"");
                 			Section4 = ifnull(translation.get("Section_4"),"");
                 			Section5 = ifnull(translation.get("Section_5"),"");
                 			Section6 = ifnull(translation.get("Section_6"),"");
                 			Section7 = ifnull(translation.get("Section_7"),"");
                 			// ===========================================
                 			// CREATE SCHEDULED TALK RECORD
                 			// ===========================================
                 			newTalk = insert into Scheduled_Talks
                 			[
                 				Added_User=zoho.loginuser
                 				Toolbox_Talks=Talk.ID
                 				File_upload=Talk.File_upload
                 				Operator=taskRow.Operator
                 				Required_Date=rec.Schedule_For_Date
                 				Section_11=Section1
                 				Section_1_Read=false
                 				Section_21=Section2
                 				Section_2_Read=false
                 				Section_31=Section3
                 				Section_3_Read=false
                 				Section_41=Section4
                 				Section_4_Read=false
                 				Section_51=Section5
                 				Section_5_Read=false
                 				Section_61=Section6
                 				Section_6_Read=false
                 				Section_71=Section7
                 				Section_7_Read=false
                 			];
                 			// ===========================================
                 			// SEND EMAIL (Using cached subject/message)
                 			// ===========================================
                 			videoURL = thisapp.A14_GetVideoURLForOperator(Talk.ID,Operator.ID);
                 			url = "https://dosca61.github.io/toolbox-video/?video=" + encodeUrl(videoURL) + "&talkid=" + newTalk;
                 			sub = ifnull(translation.get("Email_Subject"),"Toolbox Talk " + Talk.Talk + " Has been Scheduled For You");
                 			msg = ifnull(translation.get("Email_Message"),"A toolbox talk has been scheduled for you. Select the link to access your talks.");
                 			sendmail
                 			[
                 				from :zoho.loginuserid
                 				to :Operator.Email_Address
                 				cc:"admin@richmondit.ie"
                 				subject :sub
                 				message :msg + "  " + url + "&nbsp;</div>"
                 			]
                 		}
                 		// ===========================================
                 		// HANDLE COMPLETION & FREQUENCY
                 		// ===========================================
                 		if(allRowsProcessed)
                 		{
                 			freq = rec.Frequency;
                 			currentDate = rec.Schedule_For_Date;
                 			endDate = rec.Schedule_End_Date;
                 			if(freq == "Once")
                 			{
                 				rec.All_Talks_Scheduled=true;
                 			}
                 			else
                 			{
                 				nextDate = currentDate;
                 				if(freq == "Weekly")
                 				{
                 					nextDate = currentDate.addDay(7);
                 				}
                 				else if(freq == "Monthly")
                 				{
                 					nextDate = currentDate.addMonth(1);
                 				}
                 				else if(freq == "Anually")
                 				{
                 					nextDate = currentDate.addYear(1);
                 				}
                 				if(endDate != null && nextDate > endDate)
                 				{
                 					rec.All_Talks_Scheduled=true;
                 				}
                 				else
                 				{
                 					rec.Schedule_For_Date=nextDate;
                 					rec.All_Talks_Scheduled=false;
                 					for each  taskRow in rec.Talks_To_be_Scheduled
                 					{
                 						taskRow.Talk_Scheduled=false;
                 					}
                 				}
                 			}
                 		}
                 	}
                 }
				 void A03_GenerateTranslations(int talkID)
                 {
                 	// ===========================================
                 	// GENERATE TRANSLATIONS FOR ONE TALK
                 	// ===========================================
                 	Talk = Toolbox_Talks[ID == talkID];
                 	if(Talk == null)
                 	{
                 		info "Talk not found: " + talkID;
                 		return;
                 	}
                 	// Get unique languages from Operators (excluding English)
                 	allOperators = Operators[ID > 0];
                 	languageList = List();
                 	for each  op in allOperators
                 	{
                 		opLang = op.Language;
                 		if(opLang != null && opLang != "" && opLang != "English")
                 		{
                 			if(!languageList.contains(opLang))
                 			{
                 				languageList.add(opLang);
                 			}
                 		}
                 	}
                 	info "Languages to translate: " + languageList;
                 	for each  lang in languageList
                 	{
                 		// Check if translation already exists for this talk + language
                 		existingCount = Translated_Talks[Toolbox_Talk == talkID && Language == lang].count();
                 		if(existingCount > 0)
                 		{
                 			info "Skipping " + lang + " - translation already exists for: " + Talk.Talk;
                 			continue;
                 		}
                 		// STATUS: Starting translation
                 		info "Translating to: " + lang;
                 		// Build section translations
                 		Section1 = "";
                 		Section2 = "";
                 		Section3 = "";
                 		Section4 = "";
                 		Section5 = "";
                 		Section6 = "";
                 		Section7 = "";
                 		Counter = 0;
                 		for each  talkRow in Talk.Instructions
                 		{
                 			Counter = Counter + 1;
                 			translatedText = thisapp.A01_Translate_Text(lang,talkRow.Instruction);
                 			if(Counter == 1)
                 			{
                 				Section1 = translatedText;
                 			}
                 			if(Counter == 2)
                 			{
                 				Section2 = translatedText;
                 			}
                 			if(Counter == 3)
                 			{
                 				Section3 = translatedText;
                 			}
                 			if(Counter == 4)
                 			{
                 				Section4 = translatedText;
                 			}
                 			if(Counter == 5)
                 			{
                 				Section5 = translatedText;
                 			}
                 			if(Counter == 6)
                 			{
                 				Section6 = translatedText;
                 			}
                 			if(Counter == 7)
                 			{
                 				Section7 = translatedText;
                 			}
                 		}
                 		// Translate email subject and message
                 		emailSubject = thisapp.A01_Translate_Text(lang,"Toolbox Talk " + Talk.Talk + " Has been Scheduled For You");
                 		emailMessage = thisapp.A01_Translate_Text(lang,"A toolbox talk has been scheduled for you. Select the link to access your talks.");
                 		// Insert new record
                 		insert into Translated_Talks
                 		[
                 			Toolbox_Talk=talkID
                 			Language=lang
                 			Section_11=Section1
                 			Section_21=Section2
                 			Section_31=Section3
                 			Section_41=Section4
                 			Section_51=Section5
                 			Section_61=Section6
                 			Section_71=Section7
                 			Email_Subject=emailSubject
                 			Email_Message=emailMessage
                 			Added_User=zoho.loginuser
                 		]
                 		// STATUS: Finished this language
                 		info "Translation Finished: " + lang;
                 	}
                 	// STATUS: All done
                 	info "All Translations Complete";
                 }
				 map A04_GetCachedTranslation(int talkID, string language)
                 {
                 	// ===========================================
                 	// GET CACHED TRANSLATION
                 	// ===========================================
                 	result = Map();
                 	Talk = Toolbox_Talks[ID == talkID];
                 	if(Talk == null)
                 	{
                 		return result;
                 	}
                 	// If English or empty, get original text
                 	if(language == "English" || language == "" || language == null)
                 	{
                 		Counter = 0;
                 		for each  talkRow in Talk.Instructions
                 		{
                 			Counter = Counter + 1;
                 			if(Counter == 1)
                 			{
                 				result.put("Section_1",talkRow.Instruction);
                 			}
                 			if(Counter == 2)
                 			{
                 				result.put("Section_2",talkRow.Instruction);
                 			}
                 			if(Counter == 3)
                 			{
                 				result.put("Section_3",talkRow.Instruction);
                 			}
                 			if(Counter == 4)
                 			{
                 				result.put("Section_4",talkRow.Instruction);
                 			}
                 			if(Counter == 5)
                 			{
                 				result.put("Section_5",talkRow.Instruction);
                 			}
                 			if(Counter == 6)
                 			{
                 				result.put("Section_6",talkRow.Instruction);
                 			}
                 			if(Counter == 7)
                 			{
                 				result.put("Section_7",talkRow.Instruction);
                 			}
                 		}
                 		result.put("Email_Subject","Toolbox Talk " + Talk.Talk + " Has been Scheduled For You");
                 		result.put("Email_Message","A toolbox talk has been scheduled for you. Select the link to access your talks.");
                 		return result;
                 	}
                 	// Look up cached translation
                 	cachedCount = Translated_Talks[Toolbox_Talk == talkID && Language == language].count();
                 	if(cachedCount == 0)
                 	{
                 		return result;
                 	}
                 	// Get matching record
                 	for each  cached in Translated_Talks[Toolbox_Talk == talkID && Language == language]
                 	{
                 		result.put("Section_1",ifnull(cached.Section_11,""));
                 		result.put("Section_2",ifnull(cached.Section_21,""));
                 		result.put("Section_3",ifnull(cached.Section_31,""));
                 		result.put("Section_4",ifnull(cached.Section_41,""));
                 		result.put("Section_5",ifnull(cached.Section_51,""));
                 		result.put("Section_6",ifnull(cached.Section_61,""));
                 		result.put("Section_7",ifnull(cached.Section_71,""));
                 		result.put("Email_Subject",ifnull(cached.Email_Subject,""));
                 		result.put("Email_Message",ifnull(cached.Email_Message,""));
                 		break;
                 	}
                 	return result;
                 }
